    "rollup-pluginutils": "^1.5.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "eslint": "^3.2.2",
    "mocha": "^3.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "source-map-support": "^0.4.2"
  }
