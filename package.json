{
  "name": "rollup-plugin-md",
  "version": "0.1.0",
  "description": "a rollup plugin for markdown files",
  "main": "dist/rollup-plugin-md.js",
  "jsnext:main": "dist/rollup-plugin-md.mjs",
  "scripts": {
    "test": "mocha test/*.js --compilers js:buble/register",
    "pretest": "npm run build",
